{
    "name": "designsensory/laravel-form-schema",
    "description": "This is my package laravel-form-schema",
    "keywords": [
        "Designsensory",
        "laravel",
        "laravel-form-schema"
    ],
    "homepage": "https://github.com/designsensory/laravel-form-schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Vittetoe",
            "email": "jvittetoe@designsensory.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.13.0",
        "spatie/laravel-sluggable": "^3.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Designsensory\\FormSchema\\": "src",
            "Designsensory\\FormSchema\\Database\\Factories\\": "database/factories"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Designsensory\\FormSchema\\FormSchemaServiceProvider"
            ],
            "aliases": {
                "FormSchema": "Designsensory\\FormSchema\\Facades\\FormSchema"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
